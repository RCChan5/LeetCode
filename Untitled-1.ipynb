   "source": [
    "def factorial(n):\n",
    "    result = 1\n",
    "    for i,_ in enumerate(range(1, n + 1), start=1):\n",
    "        result *= i\n",
    "    return result\n",
    "\n",
    "\n",
    "num = 5\n",
    "print(f\"Factorial of {num} is {factorial(num)}\")"
   ]
  }
